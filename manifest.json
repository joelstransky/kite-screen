{
  "name": "Kite Screen",
  "description": "attempt to float the video player screen",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["kitescreen.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Attempt to float this video"
  },
  "manifest_version": 2,
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },
   "options_page": "options.html"
}
